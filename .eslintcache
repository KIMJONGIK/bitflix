\src\\routes\\Search\\SearchContainer.js",["115","116"],"import React from \"react\";\nimport { moviesApi, tvApi } from \"../../api\";\n// import Poster from \"../../components/Poster\";\nimport SearchPresenter from \"./SearchPresenter\";\n\nexport default class extends React.Component {\n  state = {\n    movieResults: null,\n    tvResults: null,\n    searchTerm: \"\",\n    error: null,\n    loading: false,\n  };\n\n  // submit\n  handleSubmit = (event) => {\n    //  form 에서 submit이 작동은 하는데, 원래 하고자 했던 이벤트를 취소한다.\n\n    event.preventDefault();\n    const { searchTerm } = this.state;\n\n    if (searchTerm !== \"\") {\n      this.searchByTerm();\n    }\n  };\n\n  // update - onChange에서 일어날 이벤트\n  updateTerm = (event) => {\n    const {\n      target: { value: searchTerm },\n    } = event;\n    this.setState({ searchTerm });\n  };\n\n  // search -> 검색어를 넣은 api 호출\n  // 이벤트가 일어났을 때 비동기로 호출\n  searchByTerm = async () => {\n    // 1. 검색어 가져오기\n    const { searchTerm } = this.state;\n\n    // 2. loading 설정하기\n    this.setState({ loading: true });\n\n    // 3. 실제 api 호출하기\n    // 데이터의 응답 형태 : { data : { results : [ {}, {}, {}, ... ] } }\n    try {\n      // moviesApi.search -> movieResults에 담기\n      const {\n        data: { results: movieResults },\n      } = await moviesApi.search(searchTerm);\n\n      // tvApi.search -> tvResults에 담기\n      const {\n        data: { results: tvResults },\n      } = await tvApi.search(searchTerm);\n\n      this.setState({ movieResults, tvResults });\n    } catch (error) {\n      this.setState({\n        error: \"결과를 찾을 수 없습니다.\",\n      });\n    } finally {\n      this.setState({\n        loading: false,\n      });\n    }\n  };\n  constructor(props) {\n    super(props);\n  }\n  // 함수형 컴포넌트에서 return에 해당된다.\n  render() {\n    const { movieResults, tvResults, searchTerm, error, loading } = this.state;\n\n    return (\n      <SearchPresenter\n        movieResults={movieResults}\n        tvResults={tvResults}\n        searchTerm={searchTerm}\n        error={error}\n        loading={loading}\n        handleSubmit={this.handleSubmit}\n        updateTerm={this.updateTerm}\n      />\n    );\n  }\n}\n","C:\\Users\\R35\\Desktop\\bitflix\\src\\routes\\Search\\SearchPresenter.js",[],"C:\\Users\\R35\\Desktop\\bitflix\\src\\api.js",[],"C:\\Users\\R35\\Desktop\\bitflix\\src\\components\\Loader.js",[],"C:\\Users\\R35\\Desktop\\bitflix\\src\\components\\Poster.js",[],"C:\\Users\\R35\\Desktop\\bitflix\\src\\components\\Message.js",[],"C:\\Users\\R35\\Desktop\\bitflix\\src\\components\\Section.js",[],"C:\\Users\\R35\\Desktop\\bitflix\\src\\routes\\Wish\\index.js",[],"C:\\Users\\R35\\Desktop\\bitflix\\src\\routes\\Wish\\WishContainer.js",["117","118","119","120"],"C:\\Users\\R35\\Desktop\\bitflix\\src\\routes\\Wish\\WishPresenter.js",["121","122"],{"ruleId":"123","replacedBy":"124"},{"ruleId":"125","replacedBy":"126"},{"ruleId":"127","severity":1,"message":"128","line":79,"column":7,"nodeType":"129","endLine":79,"endColumn":41},{"ruleId":"130","severity":1,"message":"131","line":88,"column":7,"nodeType":"129","endLine":88,"endColumn":22},{"ruleId":"132","severity":1,"message":"133","line":12,"column":1,"nodeType":"134","endLine":29,"endColumn":3},{"ruleId":"132","severity":1,"message":"135","line":5,"column":1,"nodeType":"134","endLine":64,"endColumn":2},{"ruleId":"136","severity":1,"message":"137","line":14,"column":3,"nodeType":"138","messageId":"139","endLine":16,"endColumn":4},{"ruleId":"132","severity":1,"message":"135","line":7,"column":1,"nodeType":"134","endLine":79,"endColumn":2},{"ruleId":"136","severity":1,"message":"137","line":19,"column":3,"nodeType":"138","messageId":"139","endLine":21,"endColumn":4},{"ruleId":"140","severity":1,"message":"141","line":54,"column":6,"nodeType":"142","endLine":54,"endColumn":8,"suggestions":"143"},{"ruleId":"140","severity":1,"message":"144","line":68,"column":6,"nodeType":"142","endLine":68,"endColumn":8,"suggestions":"145"},{"ruleId":"132","severity":1,"message":"135","line"